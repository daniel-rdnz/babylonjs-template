{
  "name": "slumber_party",
  "version": "1.0.0",
  "description": "experiment using babylonjs",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "webpack serve --mode development --entry ./src/game.js",
    "start-host": "webpack serve --mode development --entry ./src/game.js --host 192.168.1.9",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --entry ./src/game.js --output-path=dist --output-filename='game.js'"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^root(.*)$": "<rootDir>/src$1"
    }
  },
  "keywords": [
    "babylonjs"
  ],
  "author": "Daniel Ordo√±ez",
  "license": "ISC",
  "dependencies": {
    "babylonjs": "^4.2.0",
    "cannon": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "enzyme-to-json": "^3.6.2",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-minimizer-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.5",
    "jest-canvas-mock": "^2.3.1",
    "jsdom": "^16.6.0",
    "mini-css-extract-plugin": "^1.6.1",
    "terser-webpack-plugin": "^5.1.4",
    "webfont-webpack-plugin": "^1.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
